<div class="card-category text-center" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(data:image/jpeg;base64,)">
  Save any kind of movies <br>
  Classic
</div>

<div class="container mt-5 general-card">
  <div class="d-flex justify-content-between align-items-center">
    <h1>My List</h1>
    <%= link_to "Create a movie list", new_list_path, class: "btn btn-success" %>
  </div>
  <div class="row">
    <% @lists.each do |list| %>
    <div class="card-1 mx-2 col-3">
      <%= link_to list.name, list_path(list), class: "" %>
    </div>
    <% end %>
  </div>
</div>














